{
    "manifest_version": 3,
    "name": "GORewrite",
    "version": "2.0.3",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {},
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "side_panel": {
        "default_path": "index.html"
    },
    "permissions": [
        "clipboardWrite",
        "sidePanel",
        "downloads"
    ]
}