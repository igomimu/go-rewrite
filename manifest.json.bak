{
    "manifest_version": 3,
    "name": "GORewrite",
    "version": "1.1",
    "icons": {
        "16": "public/icons/icon16.png",
        "48": "public/icons/icon48.png",
        "128": "public/icons/icon128.png"
    },
    "action": {
        "default_title": "GORewrite"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "side_panel": {
        "default_path": "index.html"
    },
    "permissions": [
        "clipboardWrite",
        "sidePanel",
        "downloads"
    ]
}